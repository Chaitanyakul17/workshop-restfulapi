/**
 * Implements hook_help().
 */
function intelligent_tools_help($path, $arg) {
  switch ($path) {
    case 'help.page.intelligent_tools':
      return '' . t("This Module maintains 3 submodules which do NLP related task in Drupal 8. Tasks like: ") . '<a href="intelligent_tools_auto_tag">Auto Tag</a>, <a href="intelligent_tools_text_summarize">Text Summarize</a>, <a href="intelligent_tools_duplicate_rate">Duplicity Rate</a>.';

    break;
  }
}
